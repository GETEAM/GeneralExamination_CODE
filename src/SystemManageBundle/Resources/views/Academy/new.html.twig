{% extends 'SystemManageBundle:Academy:layout.html.twig' %}

{% block title %}添加学院—基本信息设置—系统管理{% endblock %}

{% block loading %}正在加载添加表单{% endblock %}

{% block right -%}
    <h1>
        添加学院
        <a href="{{ path('academy_index') }}">
            <img src="{{ asset('images/manage/back.png') }}" width="12" height="12" alt="返回学院信息列表" title="返回学院信息列表">
            <span>返回学院信息列表</span>
        </a>
    </h1>

    {{ form_start(new_form) }}
        {{ form_errors(new_form) }}

        {{ form_row(new_form.academyID, {'attr': {'placeholder': '请输入学院代号', 'autocomplete': 'off'} }) }}
        {{ form_row(new_form.academyName, {'attr': {'placeholder': '请输入学院名称', 'autocomplete': 'off'} }) }}
        {{ form_row(new_form.save, {'attr': {'class': 'btn add-btn' } }) }}

    {{ form_end(new_form, {'render_rest': true}) }}

{% endblock %}
